{
  "$schema": "https://demotime.show/demo-time.schema.json",
  "title": "2 - Demo Time",
  "description": "",
  "version": 2,
  "demos": [
    {
      "title": "Website",
      "steps": [
        {
          "action": "executeVSCodeCommand",
          "url": "https://demotime.show/",
          "command": "simpleBrowser.show",
          "args": "https://demotime.show/"
        }
      ],
      "icons": {
        "start": "globe",
        "end": ""
      }
    },
    {
      "title": "Extensions",
      "steps": [
        {
          "action": "unsetPresentationView"
        },
        {
          "action": "executeVSCodeCommand",
          "command": "workbench.action.toggleActivityBarVisibility"
        },
        {
          "action": "executeVSCodeCommand",
          "command": "workbench.view.extensions"
        },
        {
          "action": "executeVSCodeCommand",
          "command": "workbench.extensions.action.clearExtensionsSearchResults"
        },
        {
          "action": "typeText",
          "content": "Demo Time",
          "insertTypingSpeed": 100
        }
      ],
      "icons": {
        "start": "window",
        "end": ""
      }
    },
    {
      "title": "Show Overview",
      "steps": [
        {
          "action": "executeVSCodeCommand",
          "command": "workbench.explorer.fileView.focus"
        },
        {
          "action": "executeVSCodeCommand",
          "path": ".demo/2-demo-time.json",
          "command": "demo-time.showOverview"
        }
      ],
      "icons": {
        "start": "file",
        "end": ""
      }
    },
    {
      "title": "Show Config UI",
      "steps": [
        {
          "action": "open",
          "command": "demo-time.openConfigEditor",
          "args": "{\n  \"2-demo-time.json\"\n}",
          "path": ".demo/2-demo-time.json"
        }
      ],
      "icons": {
        "start": "window",
        "end": ""
      }
    },
    {
      "title": "Demo Time Panel",
      "steps": [
        {
          "action": "executeVSCodeCommand",
          "command": "demo-time.treeviewFind"
        }
      ],
      "icons": {
        "start": "window",
        "end": ""
      }
    },
    {
      "title": "Demo Time Features",
      "steps": [
        {
          "action": "openSlide",
          "path": ".demo/slides/demotime/features.md"
        }
      ],
      "icons": {
        "start": "vm",
        "end": ""
      }
    }
  ],
  "engageTime": {
    "sessionId": "yxgs5c"
  }
}